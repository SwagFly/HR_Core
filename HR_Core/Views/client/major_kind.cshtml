
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>major_kind</title>
    <link rel="stylesheet"href="../../css/table.css" type="text/css">
    <script type="text/javascript" src="../../javascript/comm/comm.js"></script>
    <script src="~/Scripts/jquery-1.10.2.js"></script>
    <script type="text/javascript">
        $(function () {
            $.ajax({
                url: "/client/major_kindselect",
                type: "get",
                success: function (msg) {
                    var dt = eval("("+msg+")");
                    for (var i = 0; i < dt.length; i++) {
                    var $tr = $("<tr></tr>");
                    var $td = $('<td class="TD_STYLE2">' + dt[i]["mfk_id"] + '</td>');
                    var $td1 = $('<td class="TD_STYLE2">' + dt[i]["major_kind_id"] + '</td>');
                    var $td2 = $('<td class="TD_STYLE2">' + dt[i]["major_kind_name"] + '</td>');
                    var $td3 = $('<td class="TD_STYLE2" width="5%"><a href="major_kindDelete/' + dt[i]["mfk_id"] + '">删除</a></td>');
                    $tr.append($td1, $td2, $td3);
                    $("#tb").append($tr);
                    }
                }
            })
        });
    </script>
</head>
<body>
    <div> 
            <table width="100%">
                <tr>
                    <td>
                        <font color="black">
                            您正在做的业务是：人力资源--客户化设置--人力资源档案管理设置--职位分类设置
                        </font>
                    </td>
                </tr>
                <tr>
                    <td align="right">
                        <input type="button" value="添加" class="BUTTON_STYLE1"
                               onclick="window.location.href='major_kind_add';">
                    </td>
                </tr>
            </table>
            <table width="100%" border="1" cellpadding=0 cellspacing=1
                   bordercolorlight=#848284 bordercolordark=#eeeeee
                   class="TABLE_STYLE1" id="tb">
                <tr>
                    <td width="46%" class="TD_STYLE1">
                        职位分类编号
                    </td>
                    <td width="47%" class="TD_STYLE1">
                        职位分类名称
                    </td>
                    <td width="5%" class="TD_STYLE1">
                        删除
                    </td>
                </tr>
            </table>
    </div>
</body>
</html>
