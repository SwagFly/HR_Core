@model List<config_major_professional>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>profession_design</title>
    <link rel="stylesheet" href="../../css/table.css" type="text/css">
    <script type="text/javascript"src="../../javascript/comm/comm.js">
    </script>
</head>
<body>
    <div> 
        <form method="post" action="../customiz/customizAction!findAttribute">
            <table width="100%">
                <tr>
                    <td>
                        <font color="black">
                            您正在做的业务是：人力资源--客户化设置--人力资源档案管理设置--职称设置
                        </font>
                    </td>
                </tr>
                <tr>
                    <td align="right"></td>
                </tr>
            </table>
            <table width="100%" border="1" cellpadding=0 cellspacing=1
                   bordercolorlight=#848284 bordercolordark=#eeeeee
                   class="TABLE_STYLE1">
                <tr>
                    <td width="80%" class="TD_STYLE1">
                        职称名称
                    </td>
                    <td width="20%" class="TD_STYLE1" align="center">
                        删除
                    </td>
                </tr>
                @foreach (config_major_professional item in Model)
                {
                <tr>
                    <td class="TD_STYLE2">
                        @item.pr_name
                    </td>
                    <td class="TD_STYLE2" align="center">
                        @Html.ActionLink("删除", "profession_designDelete", new { id = item.pr_id }, new { onclick = "return confirm('亲，你确定要删除吗？')" })
                    </td>
                </tr>
                }
            </table>
        </form>
    </div>
</body>
</html>
